{
  "output_dir": "outputs/rtx3060_full_pipeline",
  "data_dir": "datasets",
  "continue_on_error": false,
  "stages": {
    "generate_curriculum": true,
    "train": true,
    "evaluate": true,
    "ablation": false,
    "analyze": true,
    "export": true
  },
  "generate_curriculum": {
    "image_size": 84,
    "seed": 42,
    "samples_multiplier": 1.0,
    "levels": [1, 2, 3, 4]
  },
  "train": {
    "experiment_name": "sseg_nase_full_pipeline_rtx3060",
    "hardware": "rtx3060",
    "max_epochs": 100,
    "seed": 42,
    "debug": false,
    "config": "configs/experiments/exp_full_pipeline_rtx3060.yaml"
  },
  "evaluate": {
    "datasets": [
      {"name": "minimagenet", "root_dir": "datasets/minimagenet", "split": "test"},
      {"name": "cifar_fs", "root_dir": "datasets/cifar_fs", "split": "test"}
    ],
    "num_ways": 5,
    "num_shots": [1, 5],
    "num_episodes": 600,
    "device": "cuda",
    "seed": 42
  },
  "export": {
    "format": ["latex", "markdown", "json", "csv"],
    "include_baselines": true
  },
  "analyze": {
    "format": "pdf"
  }
}