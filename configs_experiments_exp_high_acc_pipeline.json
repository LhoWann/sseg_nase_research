{
  "output_dir": "outputs/high_acc_pipeline",
  "data_dir": "datasets",
  "continue_on_error": false,
  "stages": {
    "generate_curriculum": false,
    "train": true,
    "evaluate": true,
    "ablation": true,
    "analyze": true,
    "export": true
  },
  "generate_curriculum": {
    "image_size": 84,
    "seed": 42,
    "samples_multiplier": 1.0,
    "levels": [1, 2, 3, 4]
  },
  "train": {
    "experiment_name": "sseg_nase_high_acc",
    "hardware": "default_gpu",
    "max_epochs": 100,
    "seed": 42,
    "debug": false,
    "config": "configs/experiments/exp_high_acc.yaml",
    "plateau_window_size": 5,
    "plateau_threshold": 0.05
  },
  "evaluate": {
    "datasets": [
      {"name": "minimagenet", "root_dir": "datasets/minimagenet", "split": "test"},
      {"name": "cifar_fs", "root_dir": "datasets/cifar_fs", "split": "test"}
    ],
    "num_ways": 5,
    "num_shots": [1, 5],
    "num_episodes": 1000,
    "device": "cuda",
    "seed": 42
  },
  "ablation": {
    "hardware": "default_gpu",
    "max_epochs": 50,
    "seed": 42,
    "ablations": [
      "FULL_PIPELINE"
    ]
  },
  "export": {
    "format": ["latex", "markdown", "json", "csv"],
    "include_baselines": true
  },
  "analyze": {
    "format": "pdf"
  }
}
